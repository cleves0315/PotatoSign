{
  "manifest_version": 3,
  "name": "my chrome dev tools",
  "version": "1.0",
  "icons": {
    "16": "images/icon-16.png",
    "32": "images/icon-32.png",
    "48": "images/icon-48.png",
    "128": "images/icon-128.png"
  },
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "permissions": ["bookmarks"],
  "content_scripts": [
    {
      "matches": ["*://*/*"],
      "css": ["content-script/main.css", "content-script/styles.css"],
      "js": ["content-script/jq.js", "content-script/index.js"]
    }
  ]
}
