{
  "name": "土豆签",
  "version": "0.0.1",
  "manifest_version": 3,
  "description": "一个帮助你实现自定义导航站的插件",
  "icons": {
    "16": "icon-16.png",
    "128": "icon-128.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["tabs", "storage"],
  "host_permissions": ["http://*/*", "https://*/*"],
  "options_ui": {
    "page": "options.html"
  },
  "action": {
    "default_title": "土豆签",
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon-16.png",
      "64": "icon-64.png",
      "128": "icon-128.png"
    }
  }
}
